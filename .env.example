# AWS Athena Configuration
ATHENA_OUTPUT_BUCKET=s3://your-bucket/athena-results/
AWS_DEFAULT_REGION=us-west-2
ATHENA_MAX_RESULTS=100
ATHENA_TIMEOUT=120

# OpenAI Configuration (or OpenAI-compatible API)
OPENAI_API_KEY=your-api-key-here

# Custom OpenAI-compatible API endpoint (optional)
# Use this if you're hosting your own embedding service
# OPENAI_BASE_URL=http://localhost:8000/v1

# Custom embedding model name (optional)
# Use this to specify which model to use for embeddings
# OPENAI_EMBEDDING_MODEL=sentence-transformers

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
